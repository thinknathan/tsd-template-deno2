{
	"name": "tsd-template-deno2",
	"version": "1.0.0",
	"description": "A minimalist Defold TypeScript template for Deno 2.x",
	"keywords": [
		"typescript",
		"lua",
		"defold"
	],
	"author": "TS-Defold Contributors (https://ts-defold.dev/)",
	"license": "MIT",
	"engines": {
		"deno": ">=2.0.0"
	},
	"scripts": {
		"resolve": "library resolve",
		"build": "tstl",
		"lint": "deno lint ./src",
		"fmt": "deno fmt ./",
		"dev": "tstl --watch"
	},
	"devDependencies": {
		"@ts-defold/library": "^0.1.1",
		"@ts-defold/tstl-userdata-sugar": "^1.0.1",
		"@ts-defold/types": "^1.2.51",
		"@typescript-to-lua/language-extensions": "^1.19.0",
		"tstl-export-to-global": "^1.0.1",
		"tstl-trim-extensions": "^1.0.3",
		"typescript": "5.8.2",
		"typescript-to-lua": "~1.31.0",
		"typescript-tstl-plugin": "^0.3.2"
	}
}
